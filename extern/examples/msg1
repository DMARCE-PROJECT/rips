          params:
            - rcl_interfaces/srv/SetParametersAtomically
  topics:
    - topic: /parameter_events
      parameters:
        - rcl_interfaces/msg/ParameterEvent
      publishers:
        - rips
      subscribers:
        - rips
    - topic: /rosout
