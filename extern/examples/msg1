event: message
fromtopic: /videocorridor
msg:
  data: 'CORRIDOR CAMERA: SEQ 2'

rawmsg: |
  AAEAABcAAABDT1JSSUR
  PUiBDQU1FUkE6IFNFUSAyAA==

context:
  nodes:
    - node: corridorcamera
      gids:
        - ec.ff.a0.8a.5a.5e.04.1b.fb.fe.13.ce.00.00.10.03.00.00.00.00.00.00.00.00
        - ec.ff.a0.8a.5a.5e.04.1b.fb.fe.13.ce.00.00.11.04.00.00.00.00.00.00.00.00
        - ec.ff.a0.8a.5a.5e.04.1b.fb.fe.13.ce.00.00.03.03.00.00.00.00.00.00.00.00
        - ec.ff.a0.8a.5a.5e.04.1b.fb.fe.13.ce.00.00.12.03.00.00.00.00.00.00.00.00
      services:
        - service: /corridorcamera/describe_parameters
          params:
            - rcl_interfaces/srv/DescribeParameters
        - service: /corridorcamera/get_parameter_types
          params:
            - rcl_interfaces/srv/GetParameterTypes
        - service: /corridorcamera/get_parameters
          params:
